    "silex/silex": "~1.1",
    "twig/twig": "1.*",
    "nicl/silex-markdown": "1.1.*",
    "guzzle/silex-provider": "1.0.*@dev",
    "monolog/monolog": "1.6.*",
    "symfony/monolog-bridge": "2.3.*",
    "easybib/silex-qafoo-provider": "~1.1.0"
  },
  "require-dev": {
    "symfony/browser-kit": "2.3.*",
    "phpmd/phpmd": "1.*",
    "pdepend/pdepend": "1.*",
    "squizlabs/php_codesniffer": "1.*",
    "phpunit/phpunit": "3.7.*"
  },
  "prefer-stable": true
