{
  "name": "easybiblabs/easybib-discover-api",
  "description": "Discover EasyBibs API",
  "license": "BSD-2-Clause",
  "authors": [
    {"name": "Anne-Julia Scheuermann", "email": "anne@lagged.biz"}
  ],
  "keywords": ["hypermedia", "api", "oauth2"],
  "autoload": {
    "psr-0": {
      "": "src/"
    },
    "classmap": ["tests/"]
